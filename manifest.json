{
    "manifest_version": 2,
    "name": "Tag AWS VPC/Role",
    "description": "AWS Role Identity Banner",
    "version": "0.1.5",
    "icons": {
        "16": "img/vpc32.png",
        "48": "img/vpc38.png",
        "128": "img/vpc128.png"
    },
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["jquery.2.4.1.min.js", "awsIdentityBanner.js"],
        "matches": ["https://console.aws.amazon.com/*", "https://*.console.aws.amazon.com/*"],
        "all_frames": true
    }]
}